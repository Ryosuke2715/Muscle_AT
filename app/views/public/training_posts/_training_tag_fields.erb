<div class='nested-fields'>
  <div class="form-inline">
    <div class='form_group field'>
        <%= f.collection_select(:training_tag, @training_tags,:id, :training_tag) %>
    </div>
    <div class="form-group">
      <input type="text" class="form-control-sm" id="kg" name="kg">
      <label for="kg">kg</label>
    </div>

    <!-- レップ入力フォーム -->
    <div class="form-group">
      <input type="text" class="form-control-sm" id="rep" name="rep">
      <label for="reps">レップ</label>
    </div>

    <!-- セット数入力フォーム -->
    <div class="form-group">
      <input type="text" class="form-control-sm" id="set" name="set">
      <label for="set">セット</label>
    </div>
  </div>
  <%= link_to_remove_association "remove training_tag", f %>
</div>